{
    "name": "neobazaar/neobazaar",
    "description": "Neobazaar main module. This contains Doctrine 2 entities and repositories",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "neobazaar", "free", "classifieds"
    ],
    "homepage": "https://github.com/kaiohken1982/Neobazaar",
    "authors": [
        {
            "name": "Sergio Rinaudo",
            "email": "kaiohken1982@hotmail.com",
            "homepage": "http://www.sergiorinaudo.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
		"doctrine/doctrine-orm-module": "0.*",
        "razor/razor": "dev-master",
		"beberlei/DoctrineExtensions": "dev-master",
        "zf-commons/zfc-base": "0.*"
    },
    "require-dev": {
		"phpunit/phpunit" : "3.7.*",
		"doctrine/data-fixtures": "1.0.*@dev",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": {
            "Neobazaar": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}